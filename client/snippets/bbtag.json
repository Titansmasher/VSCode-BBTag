{
    "$schema": "https://raw.githubusercontent.com/rebornix/vscode-project-snippet/master/snippet.json",
    "Foreach_Loop": {
        "prefix": "{foreach;",
        "body": [
            "{foreach;${2:element};${1:array};",
            "  $0",
            ""
        ],
        "description": "Foreach loop"
    },
    "For_Loop": {
        "prefix": "{for;",
        "body":[
            "{for;${1:variable};${2:initial};${3|<,<=,>,>=,==,!=|};${4:limit};${5:1};",
            "  $0",
            ""
        ],
        "description": "For loop"
    }
}